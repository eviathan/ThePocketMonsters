    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/PocketMonsters.App/bin/Debug/net8.0/PocketMonsters.App.dll",
            "args": [],
            "cwd": "${workspaceFolder}/PocketMonsters.App",
            "console": "internalConsole",
            "stopAtEntry": false,
            "logging": {
                "diagnosticsLog": {
                    "protocolMessages": false
                },
                "moduleLoad": false,
                "programOutput": true
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}